{
  "name": "freeapi-ai-enterprise",
  "version": "2.0.0",
  "description": "FreeApi - Enterprise AI Response Caching & Acceleration System",
  "main": "src/smartcache.js",
  "author": "420White,LLC",
  "license": "Enterprise",
  "keywords": ["ai", "caching", "enterprise", "openai", "anthropic", "acceleration"],
  "scripts": {
    "start": "node src/smartcache.js",
    "dashboard": "node src/dashboard.js",
    "build": "node build.js",
    "build:exe": "pkg . --target node18-win-x64 --output dist/freeapi.exe",
    "deploy:enterprise": "node enterprise-deploy.js",
    "test": "npm run test:unit && npm run test:integration && npm run test:security",
    "test:unit": "mocha tests/enterprise-security.test.js --timeout 30000 --require tests/test-config.js --grep \"describe\"",
    "test:integration": "mocha tests/enterprise-security.test.js --timeout 60000 --require tests/test-config.js --grep \"Integration Tests\"",
    "test:security": "mocha tests/enterprise-security.test.js --timeout 60000 --require tests/test-config.js --grep \"Security Stress Tests\"",
    "test:performance": "mocha tests/enterprise-security.test.js --timeout 120000 --require tests/test-config.js --grep \"Performance\"",
    "test:watch": "mocha tests/enterprise-security.test.js --watch --require tests/test-config.js",
    "test:coverage": "nyc --reporter=html --reporter=text mocha tests/enterprise-security.test.js --require tests/test-config.js",
    "test:intercept": "node test/test-interception.js",
    "lint": "eslint src/ tests/ --fix",
    "security-scan": "npm audit --audit-level high",
    "vulnerability-check": "npm audit fix",
    "dev": "concurrently \"node src/smartcache.js\" \"node src/dashboard.js\"",
    "install:android": "npm install --production --target_arch=arm64 --target_platform=android"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "open": "^10.0.3",
    "pkg": "^5.8.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "bin": "start.js",
  "pkg": {
    "targets": ["node18-win-x64", "node18-macos-x64", "node18-linux-x64"],
    "outputPath": "dist",
    "assets": [".env", "src/**/*", "node_modules/@supabase/**/*"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Marcone1983/uacx-proxy-android3"
  },
  "homepage": "https://420white.com"
}