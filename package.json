{
  "name": "smartcache-ai-enterprise",
  "version": "1.0.0",
  "description": "Enterprise-wide AI response caching and analytics",
  "main": "src/smartcache.js",
  "scripts": {
    "start": "node src/smartcache.js",
    "dashboard": "node src/dashboard.js",
    "build": "node build.js",
    "build:exe": "pkg . --target node20-win-x64 --output dist/smartcache.exe",
    "deploy:enterprise": "node enterprise-deploy.js",
    "test:intercept": "node test/test-interception.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "pkg": "^5.8.1",
    "open": "^10.0.3"
  },
  "bin": "start.js",
  "pkg": {
    "targets": ["node20-win-x64", "node20-macos-x64", "node20-linux-x64"],
    "outputPath": "dist",
    "assets": [".env", "src/**/*"]
  }
}